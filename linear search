def linear_search(arr, target):
  for i in range(len(arr)):
    if arr[i] == target:
      print(f"element found at index {i}")
      return
  print("element is not found at any index")

my_list=(1,3,5,7,9)
linear_search(my_list,5)
linear_search(my_list, 10) # Added another call to test the "not found" case
